# Qdrant Cloud credentials (required)
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key

# Qdrant collection name (default: tenant_law)
QDRANT_COLLECTION=tenant_law

# Embedding model (default: all-MiniLM-L6-v2)
EMBEDDING_MODEL=all-MiniLM-L6-v2

# OpenAI API key for chat responses (optional but recommended)
OPENAI_API_KEY=sk-...

# OpenAI model (default: gpt-4o-mini)
OPENAI_MODEL=gpt-4o-mini

# Allowed CORS origins (default: http://localhost:3000)
CORS_ORIGINS=http://localhost:3000
